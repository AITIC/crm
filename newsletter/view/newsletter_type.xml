<?xml version="1.0"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="form_newsletter_type">
      <field name="name">form.newsletter.type</field>
      <field name="model">newsletter.type</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
          <form col="4">
              <field name="name" colspan="4" />
              <field name="model" />
              <field name="email_template_id" />
              <field name="domain" colspan="4" />
              <field name="email_from" widget="email" />
              <field name="plaintext_mode" />
              <field name="group_ids" colspan="4" />
              <button name="action_show_recipient_objects" type="object" string="Show recipient objects" colspan="4" />
          </form>
      </field>
    </record>
  </data>
</openerp>
